{
    "extends": "@tsconfig/svelte/tsconfig.json",
    "include": ["src/**/*", "src/node_modules"],
    "exclude": ["node_modules/*"],
    "compilerOptions": {
        "target": "es2020",
        "module": "es2020",
        "types": ["svelte"],
        "resolveJsonModule": true,
        "allowSyntheticDefaultImports": true,
        "moduleResolution": "node",
    }
}